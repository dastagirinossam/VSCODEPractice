<aura:component implements="lightning:availableForFlowScreens,force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickActionWithOutHeader" access="global" controller="CreateUserHandler">
    
    <aura:attribute name="recordId" type="String"/>    
        <aura:attribute name="contactId" type="String"/>    
    <aura:attribute name="type" type="string" default="success" />
    <aura:attribute name="messageText" type="string" default="New User created Successfully" />
    <aura:attribute name="saved" type="Boolean" default="false" />
    <aura:attribute name="showSpinner" type="Boolean" default="false" />
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
</aura:component>